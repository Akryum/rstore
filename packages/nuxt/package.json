{
  "name": "@rstore/nuxt",
  "type": "module",
  "version": "0.1.5",
  "description": "Reactive Store for Nuxt",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/Akryum/rstore.git",
    "type": "git",
    "directory": "packages/nuxt"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev:playground": "nuxi prepare playground && nuxi dev ../playground",
    "dev:build": "nuxi build ../playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare ../playground",
    "build": "nuxi prepare playground && pnpm run prepack",
    "prepublishOnly": "pnpm run build",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/kit": "^3.15.4",
    "@rstore/shared": "workspace:^",
    "@rstore/vue": "workspace:^"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.0.0",
    "@nuxt/eslint-config": "^1.0.1",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.15.4",
    "@nuxt/test-utils": "^3.15.4",
    "@types/node": "latest",
    "eslint": "^9.20.0",
    "nuxt": "^3.15.4",
    "typescript": "~5.7.3",
    "vitest": "catalog:",
    "vue-tsc": "^2.2.0"
  }
}
